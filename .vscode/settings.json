{
  "typescript.tsdk": "./node_modules/typescript/lib",
  "search.exclude": {},
  "files.exclude": {
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/*.js.map": true,
    "**/*.d.ts": true
  },
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.insertSpaces": true
}
